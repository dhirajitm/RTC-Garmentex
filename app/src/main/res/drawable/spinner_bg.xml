<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <stroke android:width="1dp" android:color="@color/et_border" />
                    <solid android:color="@color/white" />
                    <corners android:radius="@dimen/et_corner" />
                </shape>
            </item>
            <item android:drawable="@drawable/ic_baseline_arrow_drop_down_24" android:gravity="center_vertical|end" android:right="5dp" />
        </layer-list>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="@color/et_disabled" />
            <solid android:color="@color/et_disabled" />
            <corners android:radius="@dimen/et_corner" />
        </shape>
    </item>
</selector>